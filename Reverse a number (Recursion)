#include<stdio.h>

int reversed(int num,int rev);

int main(){
    int n=1234;
    int result=reversed(n,0);
    printf("%d ",result);
    return 0;
}

int reversed(int num,int rev){
    if(num==0){
        return rev;
    }
    return reversed(num/10,rev*10+num%10);
}




def reversed(num,rev):
    if(num==0):
        return rev
    return reversed(num//10,rev*10+num%10)
    
n=12345
res=reversed(n,0)
print(res)
