#include<stdio.h>
#include<math.h>
#define INF 9999
#define n 6

struct points{
    float x;
    float y;
};

float min(float a,float b){
    return (a<b)? a : b;
}


int main(){
    struct points p[n] = {{2,3},{12,30},{40,50},{5,1},{12,10},{3,4}};
    float d = INF;
    for(int i = 0; i<n-1;i++){
        for(int j=i+1;j<n;j++){
            float dist = sqrt(pow((p[i].x - p[j].x),2) + pow((p[i].y - p[j].y),2));
            d = min(d,dist);
        }
    }
    printf("%f",d);
    return 0;
}
