C Programming:

#include<stdio.h>

int main(){
    int a[]={1,5,7,2,9,23,64,11};
    int l = sizeof(a)/sizeof(a[0]);
    int max=a[0];
    for(int i=0;i<l;i++){
        if(a[i]>max){
            max=a[i];
        }
    }
    printf("%d",max);
    return 0;
}


Python:

array=[1,6,4,3,5,65,7]
print(max(array))
