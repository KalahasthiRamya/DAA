C programming:

#include<stdio.h>

int fib(int n);

int main(){
    int num=10;
    for(int i=0;i<num;i++){
        printf("%d ",fib(i));
    }
    return 0;
}

int fib(int n){
    if(n<=1){
        return n;
    }
    return fib(n-1)+fib(n-2);
}


Python:

def fib(num):
    if(num<=1):
        return num
    return fib(num-1)+fib(num-2)
    
n=10
for i in range(0,n):
    print(fib(i)," ",end="")
